{
	"entry": [
		"./src/index.js"
	],
	"output": {
		"filename": "javascript/app.js",
		"publicPath": "/dev/"
	},
	"module": {
		"rules": [
			{
				"enforce": "pre",
				"test": {},
				"exclude": {},
				"use": {
					"loader": "eslint-loader",
					"options": {
						"cache": true,
						"emitError": true,
						"emitWarning": false
					}
				}
			},
			{
				"test": {},
				"use": "vue-loader"
			},
			{
				"test": {},
				"exclude": {},
				"use": {
					"loader": "babel-loader?cacheDirectory=true",
					"options": {
						"presets": [
							"@babel/preset-env",
							"@vue/babel-preset-jsx"
						]
					}
				}
			},
			{
				"test": {},
				"use": [
					"vue-style-loader",
					"css-loader",
					{
						"loader": "postcss-loader",
						"options": {
							"plugins": [
								null
							]
						}
					},
					"stylus-loader"
				]
			},
			{
				"test": {},
				"use": {
					"loader": "file-loader",
					"options": {
						"name": "images/[name].[ext]",
						"publicPath": "/dev/"
					}
				}
			},
			{
				"test": {},
				"use": {
					"loader": "file-loader",
					"options": {
						"name": "fonts/[name].[ext]",
						"publicPath": "/dev/"
					}
				}
			},
			{
				"test": {},
				"use": {
					"loader": "file-loader",
					"options": {
						"name": "audio/[name].[ext]",
						"publicPath": "/dev/"
					}
				}
			}
		]
	},
	"resolve": {
		"extensions": [
			".js",
			".jsx",
			"vue"
		]
	},
	"plugins": [
		{},
		{
			"options": {
				"template": "E:\\Study\\webpack_config\\index.html",
				"filename": "html/index.html",
				"hash": false,
				"inject": true,
				"compile": true,
				"favicon": false,
				"minify": false,
				"cache": true,
				"showErrors": true,
				"chunks": "all",
				"excludeChunks": [],
				"chunksSortMode": "auto",
				"meta": {},
				"title": "Webpack App",
				"xhtml": false
			}
		},
		{
			"options": {},
			"fullBuildTimeout": 200,
			"requestTimeout": 10000
		}
	],
	"stats": {
		"colors": true,
		"warnings": false
	},
	"mode": "development",
	"devtool": "@#eval",
	"devServer": {
		"host": "localhost",
		"port": 10086,
		"publicPath": "/dev/",
		"hot": true,
		"index": "index.html",
		"historyApiFallback": {
			"rewrites": [
				{
					"from": {},
					"to": "index.html"
				}
			]
		},
		"inline": true,
		"open": true,
		"openPage": "dev/html/index.html",
		"progress": true,
		"quiet": false,
		"compress": true,
		"watchOptions": {
			"poll": false
		},
		"clientLogLevel": "warning",
		"proxy": {}
	}
}